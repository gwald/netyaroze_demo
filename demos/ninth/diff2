                        {
                        thisSection->thetaY = 0;
                        if (dy == 0)            // no theta-x rotation: this section same coord as one before
                                {
                                //printf("\nno rotation at all\n\n");
                                thisSection->thetaX = 0;
                                thisSection->thetaY = 0;
                                }
                        else             // just a theta-x rotation: find how much
                                {
                                assert(dz > 0);         // angle < PI/2

