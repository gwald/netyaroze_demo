
#////////////////////////////////////////////////////////////////////
# Makefile				
#////////////////////////////////////////////////////////////////////

#////////////////////////////////////////////////////////////////////
#				Compiler and Linker flags
#////////////////////////////////////////////////////////////////////
CFLAGS  = -O3 -g
LINKER  = -Xlinker -Map -Xlinker mapfile -Xlinker -Ttext -Xlinker 80140000
RM	= rm


PROG = main
OBJS = main.o

all: $(PROG)


$(PROG): $(OBJS) 
	$(CC) $(OBJS) $(LINKER) -o $(PROG)

main.o:    main.c cntrl.h main.h
